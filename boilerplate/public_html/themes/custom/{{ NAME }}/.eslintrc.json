{
  "extends": [
    "plugin:vue/recommended",
    "../../../.eslintrc.json"
  ],
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 2017
  },
  "rules": {
    "object-curly-newline": [
      2,
      {
        "ObjectExpression": {
          "minProperties": 4,
          "multiline": true,
          "consistent": true
        },
        "ObjectPattern": {
          "consistent": false
        }
      }
    ],
    "no-mutable-exports": 0,
    "import/extensions": [
      2,
      {
        "vue": "never"
      }
    ],
    "import/no-mutable-exports": 1,
    "import/no-extraneous-dependencies": [
      2,
      {
        "devDependencies": [
          "**/webpack.config.js",
          "**/webpack.config.*.js",
          "**/gulpfile.js",
          "**/postcss.config.js"
        ],
        "optionalDependencies": false
      }
    ],
    "import/no-unresolved": 0
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "./build/webpack.config.dev.js"
      }
    }
  }
}
