{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "sourceType": "module"
  },
  "globals": {
    "BUNDLE_TYPE": true,
    "__webpack_public_path__": true
  },
  "rules": {
    "import/no-unresolved": ["error", {
      "commonjs": true,
      "caseSensitive": true,
      "ignore": ["^(drupalSettings|js-cookie)$", "^Drupal(/.+|$)"]
    }]
  }
}
