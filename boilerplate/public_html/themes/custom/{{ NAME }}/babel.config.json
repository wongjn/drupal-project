{
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    ["@babel/plugin-transform-runtime", { "useESModules": true }]
  ],
  "browserslistConfigFile": ".browserslistrc",
  "env": {
    "legacy": { "presets": [["@babel/preset-env", { "modules": false }]] },
    "modern": {
      "presets": [
        [
          "@babel/preset-env",
          { "modules": false, "targets": { "esmodules": true } }
        ]
      ]
    }
  }
}
