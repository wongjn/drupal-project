# Ignore files and directories generated by Composer (including drupal/core-composer-scaffold).
/.editorconfig
/.gitattributes
/public_html/*
/vendor/

# Include custom modules.
!/public_html/modules/
/public_html/modules/*
!/public_html/modules/custom/

# Include custom themes.
!/public_html/themes/
/public_html/themes/*
!/public_html/themes/custom/

# Include custom profiles.
!/public_html/profiles/
/public_html/profiles/*
!/public_html/profiles/custom/

# Include custom example files.
!/public_html/sites/
/public_html/sites/*
!/public_html/sites/development.services.yml
!/public_html/sites/example.settings.local.php
